[user]
name = it3ration
email = it3ration

[credential]
helper = osxkeychain

[color]
ui = true
diff = true

[alias]
ci = commit
st = status
co = checkout
br = branch
lg = log --graph --color --pretty=format:'%C(bold red)%h%C(reset) %s (%C(green)%cr%C(reset)) %C(magenta)%an%C(reset)%C(yellow)%d%C(reset)'

[core]
editor = emacs

[push]
default = simple
